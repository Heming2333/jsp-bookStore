<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE 	hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.hdu.model.Finance" table="finance">
		<composite-id>
			<key-property name="s_id" column="s_id"></key-property>
			<key-property name="f_time" column="f_time"></key-property>
		</composite-id>
		
		<property name="f_content" />
		<property name="f_type" />
		<property name="f_money" />


	</class>
</hibernate-mapping>