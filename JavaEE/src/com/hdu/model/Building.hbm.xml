<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE 	hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.hdu.model.Building" table="building">
		<id name="dno" column="d_no">
			<generator class="identity"></generator>
		</id>
		<property name="bno"></property>
		<property name="bsex"></property>
		<property name="bintro"></property>
		<property name="dgood"></property>
		<property name="sbed"></property>
	</class>
</hibernate-mapping>